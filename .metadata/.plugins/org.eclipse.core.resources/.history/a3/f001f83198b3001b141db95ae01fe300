package Locators;

import org.openqa.selenium.By;
import org.openqa.selenium.firefox.FirefoxDriver;

public class Relativexpath {

	public static void main(String[] args)
	{
		FirefoxDriver driver=new FirefoxDriver();
		
		driver.get("file:///D:/HTML%20Code/Absolutexpath.html");
		
		driver.manage().window().maximize();
		
		//Last name
		
		driver.findElement(By.xpath("//div[1]/input[3]")).sendKeys("Ramesh");
		
		
		//Phone no
		
		driver.findElement(By.xpath("//div[2]/input[2]")).sendKeys("7896541230");
		
		
		//Link3
		
		
		driver.findElement(By.xpath("//div[3]/a[2]")).click();
		
	}

}
